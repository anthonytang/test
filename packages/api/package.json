{
  "name": "@studio/api",
  "version": "1.0.0",
  "description": "Client-safe API communication utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "studio",
    "api"
  ],
  "author": "Studio Team",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "@azure/storage-blob": "^12.17.0",
    "@studio/core": "workspace:*",
    "pg": "^8.11.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "@vitest/coverage-v8": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  }
}
