{
  "name": "@studio/auth",
  "version": "1.0.0",
  "description": "Authentication and authorization utilities",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./hooks": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "studio",
    "auth"
  ],
  "author": "Studio Team",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-browser": "^2.38.0",
    "@azure/msal-react": "^1.4.0",
    "@studio/core": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "react": "^18.0.0"
  },
  "peerDependencies": {
    "next": "^14.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^2.0.0",
    "jsdom": "^24.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0"
  }
}
