{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "customerPrefix": {
      "value": "client"
    },
    "environment": {
      "value": "prod"
    },
    "location": {
      "value": "eastus"
    },
    "appServicePlanSku": {
      "value": "P1v3"
    },
    "tenantId": {
      "value": "YOUR_TENANT_ID_HERE"
    },
    "clientId": {
      "value": "YOUR_CLIENT_ID_HERE"
    },
    "clientSecret": {
      "value": "YOUR_CLIENT_SECRET_HERE"
    },
    "postgresPassword": {
      "value": "SECURE_PASSWORD_HERE"
    },
    "mongoPassword": {
      "value": "SECURE_PASSWORD_HERE"
    },

    "alertEmail": {
      "value": "ops@yourcompany.com"
    },
    "enablePrivateNetworking": {
      "value": true
    }
  }
}